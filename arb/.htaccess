#### кеш браузера ##############
<ifModule mod_expires.c>

ExpiresActive On
 
#по умолчанию кеш в 1 день
#ExpiresDefault "access plus 1 days"

# Включаем кэширование css, javascript и текстовых файлов на одну неделю
ExpiresByType text/css "access plus 8213126400 seconds"
ExpiresByType text/javascript "access plus 8213126400 seconds"
ExpiresByType application/javascript "access plus 8213126400 seconds"
ExpiresByType application/x-javascript "access plus 8213126400 seconds"
 
# Включаем кэширование html и htm файлов на один день
ExpiresByType text/html "access plus 8213126400 seconds"

</ifModule>
######## end/кеш браузера############### 